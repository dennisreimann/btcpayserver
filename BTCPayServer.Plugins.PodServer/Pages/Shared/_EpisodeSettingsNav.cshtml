@model (string PodcastId, string EpisodeId)

<nav id="SectionNav">
    <div class="nav">
        <a id="SectionNav-@PodServerNavPages.Episodes.ToString()" class="nav-link @ViewData.IsActivePage(PodServerNavPages.Episodes)" asp-page="/Episodes/Edit" asp-route-podcastId="@Model.PodcastId" asp-route-episodeId="@Model.EpisodeId">Episode</a>
        <a id="SectionNav-@PodServerNavPages.Contributions.ToString()" class="nav-link @ViewData.IsActivePage(PodServerNavPages.Contributions)" asp-page="/Contributions/Index" asp-route-podcastId="@Model.PodcastId" asp-route-episodeId="@Model.EpisodeId">Contributions</a>
    </div>
</nav>

